{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "dev:backend": "cd server && npm start",
    "dev:full": "concurrently -k \"npm run dev\" \"npm run dev:backend\"",
    "dev:desktop": "concurrently -k \"vite\" \"wait-on tcp:5173 && electron .\"",
    "build": "vite build && node scripts/precompress.cjs",
    "precompress": "node scripts/precompress.cjs",
    "check:bundle": "npm run build && bundlesize",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json"
  },
  "config": {
    "bundlesize": [
      { "path": "dist/assets/react-vendor-*.js", "maxSize": "220kb" },
      { "path": "dist/assets/supabase-vendor-*.js", "maxSize": "160kb" },
      { "path": "dist/assets/icons-vendor-*.js", "maxSize": "40kb" },
      { "path": "dist/assets/index-*.js", "maxSize": "60kb" }
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "electron": "^30.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "wait-on": "^7.0.1",
    "bundlesize": "^1.0.0"
  }
}
